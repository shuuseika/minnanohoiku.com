    "sass": "node-sass --watch --include-path scss _themes/default/src/sass/ -o _themes/default/src/postcss/ --source-map-root file://${PWD}/ --source-map-embed true",
    "bs": "browser-sync start --config bs-config.js",
    "fractal": "fractal start --sync --port 4000",
    "develope": "run-p bs fractal sass postcss"
